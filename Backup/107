                    <p class="cus-section-107-heading mb-1">
                        <a class="d-inline-block text-decoration-none fs-4 fw-bold text-black lh-base line-2 transition text-red-hover" href="{{HeadingLink}}">{{Heading}}</a>
                    </p>
                    <p class="cus-section-107-date d-inline-block text-decoration-none fs-5_1 text-muted lh-1 mb-2" data-by="{{By}}">
                        <i class="fa fa-calendar" aria-hidden="true"></i> {{IssueDate}}
                    </p>
                    <p class="cus-section-107-body line-4 text-gray fs-5 mb-2 lh-base">{{Body}}</p>
                    <p class="cus-section-107-more">
                        <a class="d-inline-block text-decoration-none fs-5 text-dark lh-1 transition text-red-hover" href="{{HeadingLink}}">{{more}} <i class="fa fa-arrow-circle-right"></i></a>
                    </p>
                </div>
            </div>
        {{Else}}
            <div class="cus-section-107-archive mb-4 mb-lg-2 mx-0 pb-2 border-bottom row hpe-in-section" style="display: none;" data-article="{{ArticleId}}" data-url="{{HeadingLink}}" data-menu="{{Menu}}" data-submenu="{{SubMenu}}" data-date="{{MonthI}}_{{DayI}}_{{YearI}}">
                <div class="col-lg-5 mb-2 mb-lg-0 px-0">
                    <a href="{{HeadingLink}}">
                        <div class="ratio-3-2 cus-zoom" data-cus-src="{{HeadingImage}}">
                            <div class="img-content">
                                <img src="{{HeadingImage}}" alt="{{HeadingImageAlt}}" class="img-fluid" />
                            </div>
                        </div>
                </a>
                </div>
                <div class="col-lg-7 px-0 px-lg-2">
                    <p class="cus-section-107-heading mb-1">
                        <a class="d-inline-block text-decoration-none fs-4 fw-bold text-black lh-base line-2 transition text-red-hover" href="{{HeadingLink}}">{{Heading}}</a>
                    </p>
                    <p class="cus-section-107-date d-inline-block text-decoration-none fs-5_1 text-muted lh-1 mb-2">
                        <i class="fa fa-calendar" aria-hidden="true"></i> {{IssueDate}}
                    </p>
                    <p class="cus-section-107-body line-4 text-gray fs-5 mb-2 lh-base">{{Body}}</p>
                    <p class="cus-section-107-more">
                        <a class="d-inline-block text-decoration-none fs-5 text-dark lh-1 transition text-red-hover" href="{{HeadingLink}}">{{more}} <i class="fa fa-arrow-circle-right"></i></a>
                    </p>
                </div>
            </div>
        {{EndIf}}
        {{If TotalArticlesArchive=0}}
            <style>.DisplayFlag{display:none;}</style>
        {{EndIf}}
        {{If Counter=TotalArticlesPage}}
            <div class="cus-section-107-archivemore DisplayFlag text-center position-relative border-bottom" style="{{SubMenuNoRepeatCSSDisplay}}">
                <span style="cursor:pointer" class="cus-section-107-load-more border rounded fw-bold lh-lg px-5 mt-3 mb-5 d-inline-block text-gray shadow-sm">
                    <more class="hpe-custom-text-9">
                    <script type="text/javascript">Section9();</script>
                </more> {{SubMenu}}</span>
                <center style="top: 0;left: 50%;transform: translateX(-50%);" class="cus-section-107-load-more-i d-none mt-4 position-absolute"><i aria-hidden="true" class="fa fa-circle-o-notch fa-spin text-red fs-3"></i></center>
            </div>
        {{EndIf}}
    {{RepeatEnd}}
</div>
<style>
    .cus-section-107-archivemore span:hover {color: #dd0707 !important;border-color: #dd0707 !important;}
    .cus-section-107-archivemore-active ~ .cus-section-107-archive {display: flex !important;}
    .cus-section-107-archivemore-active {display: none !important;}
    div[data-cus-src*="lib/images/blank.png"]::after {position: absolute;top: 50%;left: 50%;transform: translate(-50%,-50%);color: gray;font-weight: bold;font-size: 1.5rem;width: 100%;text-align: center;padding: 0 1rem;}
</style>
<script>
    /* SubMenu Title */
    if(ReadSubmenu() == {{SubMenuId}} && ReadSubmenu() != ReadSParameters()) {
        document.title = "{{SubMenu}}";
    }
    /* Load More Button */
    if(document.getElementsByClassName("cus-section-107-archive").length == 0) {
        document.getElementsByClassName("cus-section-107-archivemore")[0].classList.add("d-none");
    }
    if(document.getElementsByClassName('cus-section-107-load-more').length) {
        document.getElementsByClassName('cus-section-107-load-more')[0].addEventListener('click', function(){
            document.getElementsByClassName("cus-section-107-load-more")[0].classList.add("invisible");
            setTimeout(function(){
                document.getElementsByClassName("cus-section-107-load-more-i")[0].classList.remove("d-none");
            },100);
            setTimeout(function(){
                document.getElementsByClassName("cus-section-107-load-more")[0].classList.remove("invisible");
                document.getElementsByClassName("cus-section-107-load-more-i")[0].classList.add("d-none");
                document.getElementsByClassName('cus-section-107-archivemore')[0].classList.add('cus-section-107-archivemore-active');
            },1200);
        });
    }
    /* Blank Image Text */
    var cus_domain = "{{PutDomain}}";
    if (cus_domain.split('/').length > 4) {
        cus_domain = cus_domain.split('://').pop().split('/').slice(1).join('/').split('/').shift();
    } else {
        cus_domain = cus_domain.split('://').pop().split('/').shift();
    }
    document.getElementsByClassName("cus-section-107-container")[0].insertAdjacentHTML("afterend", "<style>div[data-cus-src*='lib/images/blank.png']::after {content: '" + cus_domain + "';</style>");
</script>
