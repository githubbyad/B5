                        <div class="ratio-3-2 cus-zoom" data-cus-src="{{HeadingImage}}">
                            <div class="img-content">
                                <img src="{{HeadingImage}}" alt="{{HeadingImageAlt}}" class="img-fluid" />
                            </div>
                        </div>
                   </a>
                </div>
