        </span>
    </div>
    {{RepeatBegin}}
        {{If Counter<=TotalArticlesPage}}
            <div class="cus-section-107-block mb-4 mb-lg-2 mx-0 pb-2 border-bottom row hpe-in-section" data-article="{{ArticleId}}" data-url="{{HeadingLink}}" data-menu="{{Menu}}" data-submenu="{{SubMenu}}" data-date="{{MonthI}}_{{DayI}}_{{YearI}}">
                <div class="col-lg-5 mb-2 mb-lg-0 px-0">
