<script>
/* title */
document.title = document.getElementsByClassName("cus-directory-107-name")[0].innerText;
/* label */
document.getElementsByClassName("cus-directory-107-category-label")[0].innerText = document.getElementsByClassName("cus-directory-107-category-hidden")[0].innerText;
document.getElementsByClassName("cus-directory-107-category-label")[0].setAttribute("href",document.getElementsByClassName("cus-directory-107-category-hidden")[0].getAttribute("data-href"));
document.getElementsByClassName("cus-directory-107-back")[0].setAttribute("href",document.getElementsByClassName("cus-directory-107-category-hidden")[0].getAttribute("data-href"));
/* website */
var wst = document.getElementsByClassName("cus-directory-107-site")[0];
wst.innerText = wst.innerText.split("://").pop();
wst.innerText = wst.innerText.split("www.").pop();
if(wst.getAttribute("href").indexOf("http") == -1) {
wst.setAttribute("href", "http://" + wst.getAttribute("href"));
}
/* map */
var map_address = document.getElementsByClassName('cus-directory-107-address')[0].innerText.replace(/\s+/g, "%20");
map_address = (map_address == "") ? "" : map_address + ",";
var map_city = document.getElementsByClassName('cus-directory-107-city')[0].innerText.replace(/\s+/g, "%20");
map_city = (map_city == "") ? "" : map_city + ",";
var map_state = document.getElementsByClassName('cus-directory-107-state')[0].innerText.replace(/\s+/g, "%20");
map_state = (map_state == "") ? "" : map_state + ",";
var map_country = document.getElementsByClassName('cus-directory-107-country')[0].innerText.replace(/\s+/g, "%20");
map_country = (map_country == "") ? "" : map_country;
var map_zip = document.getElementsByClassName('cus-directory-107-zip')[0].innerText.replace(/\s+/g, "%20");
map_zip = (map_zip == "") ? "" : "-" + map_zip;
var map_loc = map_address + map_city + map_state + map_country + map_zip;
document.getElementById('cus-directory-107-map').setAttribute('src','https://maps.google.com/maps?width=100%25&height=600&hl=en&q=' + map_loc + '&t=&z=14&ie=UTF8&iwloc=B&output=embed')
</script>
