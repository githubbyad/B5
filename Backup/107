function GetPreview<<SubMenuId>>(){document.write('
    <div id="cus-preview-<<SubMenuId>>" class="carousel slide mb-2 position-relative" data-bs-ride="carousel" data-bs-interval="false">
        <div class="d-block w-100 border-bottom border-2 border-red mb-2">
